  "main": "/dist/index.js",
  "files": [
    "dist/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "rimraf ./dist && tsc -w",
    "clean": "rimraf ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --maxWorkers=50%",
    "test:coverage": "jest --coverage",
    "watch": "npx nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyuManagement/next-safejs.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "jose",
    "jwt",
    "next",
    "nextjs",
    "react",
    "reactjs"
  ],
  "author": {
    "name": "KSJaay",
    "email": "KSJaay@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyuManagement/next-safejs/issues"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "homepage": "https://github.com/KyuManagement/next-safejs#readme",
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/jest": "^29.5.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
